{
    "name": "Hello Extensions",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "/assets/images/hello_extensions.png"
    },
    "permissions": [
          "tabs", "notifications", "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "options_page": "options.html"
  }